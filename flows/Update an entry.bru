meta {
  name: Update an entry
  type: http
  seq: 14
}

put {
  url: {{base_url}}/v2/flows/{{flowSlug}}/entries/{{entryID}}
  body: json
  auth: bearer
}

headers {
  accept: application/json
  content-type: application/json
}

auth:bearer {
  token: {{access_token}}
}

body:json {
  {
    "data": {
      "type": "entry",
      "id": "{{entryID}}",
      "FIELD1_SLUG": "value1",
      "FIELD2_SLUG": "value2"
    }
  }
}
