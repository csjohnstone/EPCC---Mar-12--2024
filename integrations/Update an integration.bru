meta {
  name: Update an integration
  type: http
  seq: 5
}

put {
  url: {{base_url}}/v2/integrations/{{integrationID}}
  body: json
  auth: bearer
}

headers {
  accept: application/json
  content-type: application/json
}

auth:bearer {
  token: {{access_token}}
}

body:json {
  {
    "data": {
      "id": "{{integrationID}}",
      "type": "integration",
      "observes": [
        "order.created"
      ]
    }
  }
}
