meta {
  name: Update Password profile
  type: http
  seq: 4
}

put {
  url: {{base_url}}/v2/authentication-realms/{{authenticationRealmId}}/password-profiles/{{passwordProfileId}}
  body: json
  auth: bearer
}

headers {
  accept: application/json
  content-type: application/json
}

auth:bearer {
  token: {{access_token}}
}

body:json {
  {
    "data": {
      "usernameFormat": "any",
      "type": "password_profile",
      "name": "updated password profile"
    }
  }
}
