meta {
  name: Update a category
  type: http
  seq: 6
}

put {
  url: {{base_url}}/v2/categories/{{categoryID}}
  body: json
  auth: bearer
}

headers {
  accept: application/json
  content-type: application/json
}

auth:bearer {
  token: {{access_token}}
}

body:json {
  {
    "data": {
      "id": "{{categoryID}}",
      "type": "category",
      "name": "Desk lamps"
    }
  }
}
