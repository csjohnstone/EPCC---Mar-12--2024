meta {
  name: Delete customer cart association
  type: http
  seq: 3
}

delete {
  url: {{base_url}}/v2/carts/{{cartID}}/relationships/customers
  body: json
  auth: bearer
}

headers {
  accept: application/json
  content-type: application/json
  x-moltin-customer-token: {{customerToken}}
}

auth:bearer {
  token: {{access_token}}
}

body:json {
  {
    "data": [
      {
        "type": "customer",
        "id": "{{customerID}}"
      }
    ]
  }
}
