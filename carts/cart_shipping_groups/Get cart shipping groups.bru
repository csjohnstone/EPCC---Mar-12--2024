}

get {
  url: {{base_url}}/v2/carts/{{cartID}}/shipping-groups
  body: none
  auth: bearer
}

headers {
  accept: application/json
  content-type: application/json
}

auth:bearer {
  token: {{access_token}}
}

tests {
