meta {
  name: Cart merge
  type: http
  seq: 9
}

post {
  url: {{base_url}}/v2/carts/{{cartID}}/items
  body: json
  auth: bearer
}

headers {
  accept: application/json
  content-type: application/json
}

auth:bearer {
  token: {{access_token}}
}

body:json {
  {
    "data": [
      {
        "type": "cart_items",
        "cart_id": "{{cartID}}"
      }
    ],
    "options": {
      "add_all_or_nothing": false
    }
  }
}
